# 데이터베이스 설정 (PostgreSQL + pgvector)
DB_HOST=localhost
DB_PORT=5432
DB_NAME=anime_recommendation_db
DB_USERNAME=postgres
DB_PASSWORD=your_password_here

# Redis 설정
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Spring 서버 연동 설정
SPRING_SERVER_URL=http://localhost:8080
SPRING_API_KEY=your_spring_api_key_here

# API 서버 설정
API_HOST=0.0.0.0
API_PORT=8000
DEBUG=true

# Kanana 임베딩 모델 설정
USE_GPU=false
MODEL_CACHE_DIR=./models
EMBEDDING_DIMENSION=768